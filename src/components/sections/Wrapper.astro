---
import { getCollection } from 'astro:content'
import Header from './Header.astro'
import SectionResolver from './SectionResolver.astro'

const sections = await getCollection('sections')
---
<Header />
{sections.map(sect => {
  return <SectionResolver sect={sect} title={sect.data.title} afterTitle={sect.data.title || ''} type={sect.data.type} />
})}
